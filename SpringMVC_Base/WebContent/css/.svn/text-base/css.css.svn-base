                line-height:16px}
select {
	font-weight: normal;
	font-family: Microsoft Yahei,simsun,verdana,helvetica,arial,sans-serif;
	font-size: 12px;
	
}                
select option {
	font-weight: normal;
	font-family: Microsoft Yahei,simsun,verdana,helvetica,arial,sans-serif;
	font-size: 12px;
	
}                
input {
    border: 1.8px solid #939393;margin: 1px;padding: 2px;backgroundColor:#FFFFFF;
    font-family: Microsoft Yahei,simsun,verdana,helvetica,arial,sans-serif;
    font-size: 12px;
    }
.txt1 {font-family: Microsoft Yahei,simsun,verdana,helvetica,arial,sans-serif;font-size: 12px;letter-spacing:1pt;background-Color:#F5F5F5;border-color:#2E71A4;}
.txt2 {font-family: Microsoft Yahei,simsun,verdana,helvetica,arial,sans-serif;text-align:right}
.btn1 {font-family: Microsoft Yahei,simsun,verdana,helvetica,arial,sans-serif;font-size: 12px;letter-spacing:1pt;background-Color:#F5F5F5;border-color:#2E71A4;cursor:hand;}
.btn2 {font-family: Microsoft Yahei,simsun,verdana,helvetica,arial,sans-serif;font-size: 12px;letter-spacing:1pt;background-Color:#F5F5F5;border-color:#2E71A4;cursor:hand;padding: 0px;margin: 1px;}
A.head1     { font-family: Microsoft Yahei,simsun,verdana,helvetica,arial,sans-serif; color: #111987; font-size: 14px; text-decoration: none;letter-spacing:1px;line-height:16pt;}
A.head1:visited { color:color: #565656; text-decoration: none;}
A.head1:hover   { color:#FF6A60; text-decoration: none;}
.head1_in { font-family: Microsoft Yahei,simsun,verdana,helvetica,arial,sans-serif; color: #111987; font-size: 14px; text-decoration: none;letter-spacing:1px;line-height:16pt;}
A.head2     { font-family: Microsoft Yahei,simsun,verdana,helvetica,arial,sans-serif; color: #0000FF; font-size: 12px; letter-spacing:1px;line-height:16pt;}
A.head2:visited { color:color: #0000FF; }
A.head2:hover   { color:#FF6A60; text-decoration: none;}

