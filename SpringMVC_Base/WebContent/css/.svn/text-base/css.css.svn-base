body {
    font-family:Microsoft Yahei,simsun,verdana,helvetica,arial,sans-serif;
    padding:20px;
    font-size:12px;
    margin:0;
}

td {
font-family: Microsoft Yahei,simsun,verdana,helvetica,arial,sans-serif;font-size:9pt;line-height:15pt;letter-spacing:1px;}
.T1 {font-size:16pt}
.T2 {font-size:14pt}
.T3 {font-size:12pt}
.red {color:red}

.menu_head1 {background-color:#FFFFFF;
                border-bottom:#F56E2C 1px solid;
                border-left:#F56E2C 1px solid;
                border-right:#F56E2C 1px solid;
